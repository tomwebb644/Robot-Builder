{
  "name": "robot-builder",
  "version": "0.1.0",
  "private": true,
  "main": "electron/main.js",
  "scripts": {
    "dev": "concurrently -k \"npm:dev:renderer\" \"npm:dev:electron\"",
    "dev:renderer": "vite",
    "dev:electron": "wait-on tcp:5173 && cross-env VITE_DEV_SERVER_URL=http://localhost:5173 electron .",
    "build": "rimraf dist && vite build && electron-builder",
    "lint": "eslint . --ext .ts,.tsx,.js,.cjs,.mjs",
    "typecheck": "tsc --noEmit"
  },
  "build": {
    "appId": "com.example.robotbuilder",
    "files": [
      "dist",
      "electron",
      "package.json"
    ],
    "extraMetadata": {
      "main": "electron/main.js"
    }
  },
  "dependencies": {
    "@react-three/drei": "^9.88.11",
    "@react-three/fiber": "^8.15.13",
    "cross-env": "^7.0.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "three": "^0.161.0",
    "zustand": "^4.5.4",
    "lucide-react": "^0.363.0",
    "classnames": "^2.5.1"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "concurrently": "^8.2.2",
    "electron": "^29.1.4",
    "electron-builder": "^24.13.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "rimraf": "^5.0.5",
    "typescript": "5.5.4",
    "vite": "^5.2.10",
    "wait-on": "^7.2.0",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0"
  }
}